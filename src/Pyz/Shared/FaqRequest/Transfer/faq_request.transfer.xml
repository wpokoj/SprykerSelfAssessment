<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="FaqCustomer">
        <property name="customerId" type="int"/>
    </transfer>

    <transfer name="FaqData">
        <property name="idFaq" type="int"/>
        <property name="question" type="string"/>
        <property name="answer" type="string"/>
        <property name="userVoted" type="bool"/>
        <property name="voteCount" type="int"/>
    </transfer>

    <transfer name="FaqDataCollection">
        <property name="faqsDate" type="FaqData[]" singular="faqData"/>
        <property name="faqCustomer" type="FaqCustomer" />
        <property name="pagination" type="Pagination" />
    </transfer>

    <transfer name="FaqVoteRequest">
        <property name="faqCustomer" type="FaqCustomer"/>
        <property name="idFaq" type="int"/>
        <property name="value" type="bool"/>
    </transfer>

</transfers>
