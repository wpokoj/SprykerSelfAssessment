<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestFaqVotesRequestAttributes">
        <property name="idFaq" type="int"/>
        <property name="voted" type="bool"/>
    </transfer>

    <transfer name="RestFaqVotesResponseAttributes">
        <property name="voted" type="bool"/>
    </transfer>

    <transfer name="FaqVote">
        <property name="idCustomer" type="int" />
        <property name="idFaq" type="int"/>
        <property name="voted" type="bool"/>
    </transfer>


    <transfer name="FaqVoteCollection">
        <property name="customerId" type="int" />
        <property name="faqs" type="FaqVoteTransfer[]" singular="faq"/>
    </transfer>

</transfers>

