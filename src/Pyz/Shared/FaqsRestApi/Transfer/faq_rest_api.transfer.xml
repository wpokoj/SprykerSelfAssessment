<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestFaqsResponseAttributes">
        <property name="question" type="string"/>
        <property name="answer" type="string"/>
        <property name="voteCount" type="int"/>
        <property name="voted" type="bool"/>
    </transfer>


    <transfer name="RestFaqsRequestAttributes">
        <property name="question" type="string"/>
        <property name="answer" type="string"/>
    </transfer>

    <transfer name="RestFaqVotesRequestAttributes">
        <property name="idFaq" type="int"/>
        <property name="voted" type="bool"/>
    </transfer>

    <transfer name="RestFaqVotesResponseAttributes">
        <property name="voted" type="bool"/>
    </transfer>

</transfers>

